{
  "clientId": "askme-analytics-app",
  "debug": false,
  "apiKey": "phc_MN5MXCec7lNZtZakqpRQZqTLaPfcV6CxeE8hfbTUFE2",
  "apiHost": "https://us.i.posthog.com",
  "autocapture": false,
  "useBuiltInPageview": true,
  "capture_pageview": true,
  "capture_pageleave": true,
  "enableCustomDomTracking": false,
  "preferCustomOverAutocapture": false,
  "emailSelectors": "input[type=\"email\"], input[name*=\"email\" i], input[placeholder*=\"email\" i], input[id*=\"email\" i]",
  "analyticsLibraryPath": "/lib/clientAnalytics/ask-me-analytics.min.js",
  "constantsPath": "/lib/clientAnalytics/ph-constants.js",
  "injectorPath": "/lib/clientAnalytics/ph-product-injector-refactored.js",

  "cartExtractorConfig": {
    "cartContainerSelector": ".pricing-page, main, body",
    "cartItemSelector": ".ph-track-product-item, [class*='pricing-card']",
    "cartItemNameSelector": ".text-2xl, h2, h3, [class*='plan-name']",
    "cartItemPriceSelector": ".text-4xl, [class*='price']",
    "cartItemQuantitySelector": "[data-quantity]",
    "currencySelector": ".currency, [class*='currency']",
    "singleItemMode": true
  },
  "steps": [
    {
      "key": "LANDING_PAGE_VIEWED",
      "url": "/",
      "urlMatch": "exact",
      "autoFire": true,
      "oncePerPath": true
    },
    {
      "key": "INFORMATIONAL_CONTENT_VIEWED",
      "url": "/demo",
      "urlMatch": "contains",
      "autoFire": true,
      "oncePerPath": true
    },
    {
      "key": "FAQ_VIEWED",
      "url": "/faq",
      "urlMatch": "contains",
      "autoFire": true,
      "oncePerPath": true
    },
    {
      "key": "CONTACT_US_VIEWED",
      "url": "/contact",
      "urlMatch": "contains",
      "autoFire": true,
      "oncePerPath": true
    },
    {
      "key": "LOGIN_CONTENT_VIEWED",
      "url": "/login",
      "urlMatch": "contains",
      "autoFire": true,
      "oncePerPath": true
    },
    {
      "key": "DASHBOARD_VIEWED",
      "url": "/dashboard",
      "urlMatch": "contains",
      "autoFire": true,
      "oncePerPath": true
    },
    {
      "key": "PRICING_PAGE_VIEWED",
      "url": "/pricing",
      "urlMatch": "contains",
      "selector": ".ph-track-pricing-view",
      "requireSelectorPresent": true,
      "autoFire": true,
      "oncePerPath": true
    },
    {
      "key": "PRODUCT_SELECTED",
      "selector": ".ph-track-product-select",
      "extractCart": true
    },
     {
      "key": "CHECKOUT_STARTED",
      "selector": ".ph-track-product-select",
      "extractCart": true
    },
    {
      "key": "CHECKOUT_COMPLETED",
      "url": "/dashboard",
      "urlMatch": "contains",
      "selector": ".ph-track-checkout-complete",
      "requireSelectorPresent": true,
      "autoFire": true,
      "oncePerPath": true
    },
    {
      "key": "SUBSCRIPTION_COMPLETED",
      "url": "/dashboard",
      "urlMatch": "contains",
      "selector": ".ph-track-checkout-complete",
      "requireSelectorPresent": true,
      "autoFire": true,
      "oncePerPath": true
    },
    {
      "key": "LOGIN_ATTEMPTED",
      "selector": ".ph-track-auth-login"
    },
    {
      "key": "LOGOUT_CLICKED",
      "selector": ".ph-track-auth-logout"
    }
  ]
}
